{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "./src/**/*",
    "src/**/*controller.ts"
  ],
  "spec": {
    "name": "Compliance API",
    "basePath": "v2",
    "outputDirectory": "public",
    "specVersion": 3,
    "specMerging": "recursive",
    "securityDefinitions": {
      "discord": {
        "type": "apiKey",
        "name": "discord",
        "in": "header"
      },
      "cloudflare": {
        "type": "apiKey",
        "name": "cloudflare",
        "in": "header"
      }
    },
    "paths": {
      "/addons": {
        "post": {
          "consumes": [
            "multipart/form-data"
          ],
          "parameters": [
            {
              "in": "formData",
              "name": "headerFile",
              "required": true,
              "type": "file"
            }
          ]
        }
      }
    }
  },
  "routes": {
    "basePath": "v2",
    "routesDir": "build",
    "authenticationModule": "src/v2/tools/authentification.ts"
  }
}