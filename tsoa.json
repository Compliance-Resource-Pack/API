{
    "entryFile": "src/index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": [
        "./src/**/*",
        "src/**/*controller.ts"
    ],
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "discord": {
                "type": "apiKey",
                "name": "discord",
                "in": "header"
            }
        }
    },
    "routes": {
        "routesDir": "build",
        "authenticationModule": "src/v2/tools/authentification.ts"
    },
    "compilerOptions": {
        "baseUrl": "/v2"
    }
}