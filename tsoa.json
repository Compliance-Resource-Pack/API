{
    "entryFile": "src/index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": [
        "./src/**/*",
        "src/**/*controller.ts"
    ],
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3
    },
    "routes": {
        "routesDir": "build"
    },
    "compilerOptions": {
        "baseUrl": "/v2"
    }
}